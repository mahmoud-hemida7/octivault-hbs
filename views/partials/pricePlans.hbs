    {{{ _editable }}}
    <section id="plans" class="section price-plan-area dark-bg overflow-hidden ptb_100">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-12 col-md-10 col-lg-7">
                    <!-- Section Heading -->
                    <div class="section-heading text-center">
                        <h2>{{title}}</h2>
                        <p class="d-none d-sm-block mt-4">
                            {{description}}
                        </p>
                    </div>
                </div>
            </div>
            <div class="row justify-content-center">
                <div class="col-12 col-sm-10 col-lg-8">
                    <div class="row price-plan-wrapper">
                        {{#each singelPricePlan}}
                        <div class="col-12 col-md-6" style="align-self: stretch">
                            <!-- Single Price Plan -->
                            <div class="single-price-plan text-center dark-bg p-5" style="height: 104%">
                                <!-- Plan Thumb -->
                                <div class="plan-thumb" style="height: 100px">
                                    <img class="price-lg" src="{{planThumbPhoto.filename}}" alt="" />
                                </div>
                                <!-- Plan Title -->
                                <div class="plan-title my-2 my-sm-3">
                                    <h3 class="text-uppercase">{{planeTitle}}</h3>
                                    {{#if comingSoon}}
                                    <span style="position: absolute; bottom: -20px; left: 1rem; font-size: 15px; width: 93%;" class="badge badge-primary">
                                        {{comingSoon}}
                                    </span>
                                    {{/if}}
                                </div>

                                <div class="plan-description">
                                    <ul class="plan-features">
                                        {{#each planDescriptionListItems}}
                                        <li class="py-2">{{listItem}}</li>
                                        {{/each}}
                                    </ul>
                                </div>
                                <!-- Plan Button -->
                                <!-- <div class="plan-button">
                                        <a href="#" class="btn appmax-btn mt-3">Get Started</a>
                                    </div> -->
                            </div>
                        </div>
                        {{/each}}
                    </div>
                </div>
            </div>
            <div class="row justify-content-center pt-5">
                <p class="text-body pt-4 fw-5">
                    {{contactUs}} <a href="{{contactUsLink.cached_url}}">Contact Us</a>
                </p>
            </div>
        </div>
    </section>