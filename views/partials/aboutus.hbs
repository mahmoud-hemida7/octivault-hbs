        {{{ _editable }}}
        <section class="section service-area dark-bg overflow-hidden ptb_100">
            <div class="container mt-5">
                <div class="row justify-content-between align-items-center">
                    <div class="col-12 col-lg-6 order-2 order-lg-1">
                        <!-- Service Text -->
                        <div class="service-text pt-4 pt-lg-0">
                            <span class="section-span">{{subTitle}}</span>
                            {{!-- TITLE --}}
                            <h2 style="font-size: 2.2em;" class="text-capitalize mb-4">{{title}}</h2>
                            {{!-- description --}}
                            <p class="mb-4">{{description}}</p>
                            {{!-- subDescription --}}
                            <p>{{subDescription}}</p>
                            {{!-- btn link --}}
                            <a href="{{contactBtn.cached_url}}" class="btn appmax-btn mt-4">{{BtnTitle}}</a>
                        </div>
                    </div>
                    <div class="col-12 col-lg-4 order-1 order-lg-2 d-none d-lg-block">
                        <!-- Service Thumb -->
                        <div class="service-thumb mx-auto">
                            <img src="{{aboutPhoto.filename}}" alt="">
                        </div>
                    </div>
                </div>
            </div>
        </section>

        