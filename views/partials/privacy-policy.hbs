        <!--====== Policy Area START ======-->
        {{{ _editable }}}
        <section class="section dark-bg faq-area ptb_100 terms">
            <div class="container mt-5">
                <div class="row justify-content-center">
                    <div class="col-12 col-md-10 col-lg-7">
                        <!-- Section Heading -->
                        <div class="section-heading text-center">
                            <h2 class="text-capitalize">{{title}}</h2>
                        </div>
                    </div>
                </div>
                <div class="row justify-content-center">
                    <div class="col-12 text-center">
                        {{#each privacyTerms}}
                                <h2>{{title}}</h2>
                            {{#each addSingleTerm}}
                                {{!-- title --}}
                                {{#if title}}
                                    <h3>{{title}}</h3>
                                {{/if}}
                                {{#if orderListItems}}
                                <ol>
                                    {{#each orderListItems}}
                                        <li>{{listItem}}</li>
                                    {{/each}}
                                </ol>
                                {{/if}}
                                {{!-- description --}}
                                {{#if description}}
                                    <p>{{description}}</p>
                                {{/if}}
                                {{!-- subDescription --}}
                                {{#if subDescription}}
                                    <p>{{subDescription}}</p>
                                {{/if}}
                            {{/each}}
                        {{/each}}
                        
                    </div>
                </div>
            </div>
        </section>
        <!--====== Terms Area End ======-->
