        {{{ _editable }}}
        <section id="contact" class="contact-area bg-gray ptb_100" style="padding-top: 150px; background: #000;">
            <div class="container">
                <div class="row justify-content-between">
                    <div class="col-12 col-lg-6 order-1 p-5">
                        <!-- Contact Us -->
                        <div class="contact-us">
                            <h2 class="mb-4">{{{contactTitle}}}</h2>
                            <ul>
                                <!-- <li class="py-3">
                                    <a class="media" href="index.html?to=">
                                        <div class="social-icon mr-3">
                                            <i class="fas fa-home"></i>
                                        </div>
                                        <span class="media-body align-self-center">
                                            <h3 class="mb-2">Location</h3>
                                            <span>677 Timpany Blvd, Gardner MA 1440</span>
                                        </span>
                                    </a>
                                </li> -->
                                {{#each contactWay}}
                                <li class="py-3">
                                    <a class="media" href="{{contactWay}}">
                                        <div class="social-icon mr-3">
                                            <i class="fas {{contactIcon}} -alt"></i>
                                        </div>
                                        <span class="media-body align-self-center">
                                            <h3 class="mb-2">{{contactTitle}}</h3>
                                            <span>{{contactWay}}</span>
                                        </span>
                                    </a>
                                </li>
                                {{/each}}
                                {{!-- <li class="py-3">
                                    <a class="media" href="mailto:info@octivault.com">
                                        <div class="social-icon mr-3">
                                            <i class="fas fa-envelope"></i>
                                        </div>
                                        <span class="media-body align-self-center">
                                            <h3 class="mb-2">Email</h3>
                                            <span>info@octivault.com</span>
                                        </span>
                                    </a>
                                </li> --}}
                            </ul>
                        </div>
                    </div>
                    <div class="col-12 col-lg-6 pr-lg-5 order-2">
                        <!-- Contact Box -->
                        <div class="contact-box overlay-dark text-center rounded p-4 p-sm-5 mt-5 mt-lg-0 shadow-lg">
                            <!-- Contact Form -->
                            <form id="contact-form" class="validateForm" method="POST" action="/api/submit-form">
                                {{#each formSection}}
                                <div class="contact-top">
                                    <h3 class="contact-title pb-3">{{messageTitle}}</h3>
                                </div>
                                <div class="row">
                                    
                                    <div class="col-12">
                                        {{#each fields}}
                                        <div class="form-group">
                                            <input type="{{type}}" id="{{name}}"  class="form-control" name="{{name}}" placeholder="{{placeholder}}" required="required">
                                        </div>
                                        {{/each}}

                                        <div class="form-group">
                                            <textarea class="form-control" id="message"  name="message" placeholder="Message" required="required"></textarea>
                                        </div>
                                    </div>

                                    <div class="col-12">
                                        <button id="submit" type="submit" class="btn btn-lg btn-yellow btn-block mt-3"><span class="pr-3"><i class="fas fa-paper-plane"></i></span>{{sendMessage}}</button>
                                    </div>
                                    
                                </div>
                                {{/each}}
                            </form>
                            <p class="form-message">
                                
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </section>